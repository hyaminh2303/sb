%ul.ui-select-choices.ui-select-choices-content.dropdown-menu{"ng-show" => "$select.items.length > 0", :role => "listbox"}
  %li.ui-select-choices-group{:id => "ui-select-choices-{{ $select.generatedId }}"}
    .divider{"ng-show" => "$select.isGrouped && $index > 0"}
    .ui-select-choices-group-label.dropdown-header{"ng-bind" => "$group.name", "ng-show" => "$select.isGrouped"}
    .ui-select-choices-row{:id => "ui-select-choices-row-{{ $select.generatedId }}-{{$index}}", "ng-class" => "{active: $select.isActive(this), disabled: $select.isDisabled(this)}", :role => "option"}
      %a.ui-select-choices-row-inner{:href => "javascript:void(0)"}